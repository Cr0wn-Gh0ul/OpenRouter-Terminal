{
  "name": "openrouter-terminal",
  "version": "1.0.1",
  "description": "A CLI tool for chatting with AI models via OpenRouter",
  "type": "module",
  "main": "dist/index.mjs",
  "bin": {
    "openrouter-terminal": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts --clean",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.cjs",
    "build:sea": "bash scripts/build-sea.sh",
    "build:sea:all": "bash scripts/build-sea-all.sh",
    "start": "node dist/index.mjs",
    "dev": "tsdown src/index.ts --format esm && node dist/index.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "openrouter",
    "ai",
    "chatbot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cr0wn-Gh0ul/OpenRouter-Terminal.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.1",
    "esbuild": "^0.24.0",
    "postject": "^1.0.0-alpha.6",
    "tsdown": "^0.20.3",
    "typescript": "latest"
  },
  "dependencies": {
    "@openrouter/sdk": "^0.5.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}